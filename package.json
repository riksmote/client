{
  "name": "task",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:worker": "ng build; npm run store:404; npm run store:worker; http-server -p 8080 -c-1 dist/task",
    "build": "ng build --configuration production; npm run store:worker;",
    "store:404": "cp ./src/index.html ./dist/task/404.html",
    "store:worker": "cp ./src/ngsw-worker.js ./dist/task",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "format": "prettier --write \"src/**/*.ts\"",
    "ghpages": "ng build --output-path docs --base-href /riksmote.github.io/"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.1.2",
    "@angular/cdk": "^12.1.2",
    "@angular/common": "~12.1.2",
    "@angular/compiler": "~12.1.2",
    "@angular/core": "~12.1.2",
    "@angular/forms": "~12.1.2",
    "@angular/material": "^12.1.2",
    "@angular/platform-browser": "~12.1.2",
    "@angular/platform-browser-dynamic": "~12.1.2",
    "@angular/router": "~12.1.2",
    "@angular/service-worker": "~12.1.2",
    "angularx-social-login": "^4.0.1",
    "autobind-decorator": "^2.4.0",
    "bootstrap": "^5.0.0-beta3",
    "jquery": "^3.6.0",
    "lodash": "^4.17.21",
    "ngx-socket-io": "^4.0.0",
    "popper.js": "^1.16.1",
    "rxjs": "~6.5.5",
    "socket.io-client": "^4.1.1",
    "sweetalert2": "^11.0.17",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.1.2",
    "@angular/cli": "~12.1.2",
    "@angular/compiler-cli": "~12.1.2",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash": "^4.14.168",
    "@types/node": "^12.11.1",
    "angular-cli-ghpages": "1.0.0",
    "codelyzer": "^6.0.0",
    "http-server": "^13.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "sass": "~1.32.12",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.3.5"
  }
}
